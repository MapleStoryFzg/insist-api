                            'json'}} <a href="javascript:void(0);">
                            <span class="glyphicon glyphicon-chevron-down"></span>
                        </a> {{else}} {{#compareParamType paramType
                            'array'}} <a href="javascript:void(0);">
                            <span class="glyphicon glyphicon-chevron-down"></span>
                        </a> {{else}} {{/compareParamType}} {{/compareParamType}}
