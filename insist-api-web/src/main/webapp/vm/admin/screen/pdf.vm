            <div class="col-sm-12">
                <table class="table table-bordered" style="table-layout: fixed">
                    <caption>请求头部</caption>
                    <thead>
                    <tr style="background-color: rgb(0, 136, 204);color: rgb(255, 255, 255);">
                        <th>名称</th>
                        <th>类型</th>
                        <th>说明</th>
                        <th>示例</th>
                        <th width="6%">操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{# if interfaceParamHeaderEntities}}
                    {{#interfaceParamHeaderEntities}}
