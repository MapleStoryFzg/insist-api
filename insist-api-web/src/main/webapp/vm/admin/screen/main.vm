<script type="text/javascript">
    $(function () {
        //HTTP请求头部参数类型选择事件
        $("#myHttpModal").find("#headerParamTable>tbody").delegate("select[name='paramTypesHeaderInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json" || $(this).val() == "array") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //HTTP请求参数类型选择事件
        $("#myHttpModal").find("#requestParamTable>tbody").delegate("select[name='paramTypesInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json" || $(this).val() == "array") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //HTTP响应参数类型选择事件
        $("#myHttpModal").find("#responseParamTable>tbody").delegate("select[name='paramTypesResponseInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json" || $(this).val() == "array") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //RPC请求参数类型选择事件
        $("#myRpcModal").find("#rmiParamTable>tbody").delegate("select[name='paramTypesInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json" || $(this).val() == "array") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //RPC响应参数类型选择事件
        $("#myRpcModal").find("#rmiResponseParamTable>tbody").delegate("select[name='paramTypesResponseInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json" || $(this).val() == "array") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //HTTP接口添加请求头部
        $("#httpAddHeaderParam").click(function () {
            $("#myHttpModal").find("#headerParamTable>tbody").append($("#httpAddHeaderParamTemplate").html());
        });

        //HTTP接口添加请求参数
        $("#httpAddRequestParam").click(function () {
            $("#myHttpModal").find("#requestParamTable>tbody").append($("#httpAddRequestParamTemplate").html());
        });

        //HTTP接口添加响应参数
        $("#httpAddResponseParam").click(function () {
            $("#myHttpModal").find("#responseParamTable>tbody").append($("#httpAddResponseParamTemplate").html());
        });

        //Rpc接口添加请求参数
        $("#rpcAddRequestParam").click(function () {
            $("#myRpcModal").find("#rmiParamTable>tbody").append($("#rpcAddRequestParamTemplate").html());
        });

        //RPC接口添加响应参数
        $("#rpcAddResponseParam").click(function () {
            $("#myRpcModal").find("#rmiResponseParamTable>tbody").append($("#rpcAddResponseParamTemplate").html());
        });

        //HTTP接口请求头部行选中事件
        $("#myHttpModal").delegate("#headerParamTable > tbody > tr.outerTr", "click", function () {
            $("#myHttpModal").find("#headerParamTable > tbody > tr.outerTr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //HTTP接口请求参数行选中事件
        $("#myHttpModal").delegate("#requestParamTable > tbody > tr.outerTr", "click", function () {
            $("#myHttpModal").find("#requestParamTable > tbody > tr.outerTr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //HTTP接口响应参数行选中事件
        $("#myHttpModal").delegate("#responseParamTable > tbody > tr.outerTr", "click", function () {
            $("#myHttpModal").find("#responseParamTable > tbody > tr.outerTr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //RPC接口请求参数行选中事件
        $("#myRpcModal").delegate("#rmiParamTable > tbody > tr.outerTr", "click", function () {
            $("#myRpcModal").find("#rmiParamTable > tbody > tr.outerTr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //RPC接口响应参数行选中事件
        $("#myRpcModal").delegate("#rmiResponseParamTable > tbody > tr.outerTr", "click", function () {
            $("#myRpcModal").find("#rmiResponseParamTable > tbody > tr.outerTr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //HTTP接口请求头部删除行事件
        $("#myHttpModal").delegate("#headerParamTable .glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().next().remove();
            $(this).parent().parent().parent().remove();
        });

        //HTTP接口请求参数删除行事件
        $("#myHttpModal").delegate("#requestParamTable .glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().next().remove();
            $(this).parent().parent().parent().remove();
        });

        //HTTP接口响应参数删除行事件
        $("#myHttpModal").delegate("#responseParamTable .glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().next().remove();
            $(this).parent().parent().parent().remove();
        });

        //RPC接口请求参数删除行事件
        $("#myRpcModal").delegate("#rmiParamTable .glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().next().remove();
            $(this).parent().parent().parent().remove();
        });

        //RPC接口响应参数删除行事件
        $("#myRpcModal").delegate("#rmiResponseParamTable .glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().next().remove();
            $(this).parent().parent().parent().remove();
        });

        //HTTP接口请求头部上移事件
        $("#httpParupHeader").click(function () {
            var currentElement = $("#myHttpModal").find("#headerParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.prev().prev().length > 0) {
                nextElement.insertBefore(currentElement.prev().prev());
                currentElement.insertBefore(nextElement);
            }
        });

        //HTTP接口请求参数上移事件
        $("#httpParup").click(function () {
            var currentElement = $("#myHttpModal").find("#requestParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.prev().prev().length > 0) {
                nextElement.insertBefore(currentElement.prev().prev());
                currentElement.insertBefore(nextElement);
            }
        });

        //HTTP接口响应参数上移事件
        $("#httpParupResponse").click(function () {
            var currentElement = $("#myHttpModal").find("#responseParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.prev().prev().length > 0) {
                nextElement.insertBefore(currentElement.prev().prev());
                currentElement.insertBefore(nextElement);
            }
        });

        //RPC接口上移事件
        $("#rpcParup").click(function () {
            var currentElement = $("#myRpcModal").find("#rmiParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.prev().prev().length > 0) {
                nextElement.insertBefore(currentElement.prev().prev());
                currentElement.insertBefore(nextElement);
            }
        });

        $("#rpcParupResponse").click(function () {
            var currentElement = $("#myRpcModal").find("#rmiResponseParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.prev().prev().length > 0) {
                nextElement.insertBefore(currentElement.prev().prev());
                currentElement.insertBefore(nextElement);
            }
        });

        //HTTP接口请求头部下移事件
        $("#httpPardownHeader").click(function () {
            var currentElement = $("#myHttpModal").find("#headerParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.next().next().next().length > 0) {
                currentElement.insertAfter(currentElement.next().next().next());
                nextElement.insertAfter(currentElement);
            }
        });

        //HTTP接口请求参数下移事件
        $("#httpPardown").click(function () {
            var currentElement = $("#myHttpModal").find("#requestParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.next().next().next().length > 0) {
                currentElement.insertAfter(currentElement.next().next().next());
                nextElement.insertAfter(currentElement);
            }
        });

        //HTTP接口响应参数下移事件
        $("#httpPardownResponse").click(function () {
            var currentElement = $("#myHttpModal").find("#responseParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.next().next().next().length > 0) {
                currentElement.insertAfter(currentElement.next().next().next());
                nextElement.insertAfter(currentElement);
            }
        });

        //RPC接口请求参数下移事件
        $("#rpcPardown").click(function () {
            var currentElement = $("#myRpcModal").find("#rmiParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.next().next().next().length > 0) {
                currentElement.insertAfter(currentElement.next().next().next());
                nextElement.insertAfter(currentElement);
            }
        });

        //RPC接口响应参数下移事件
        $("#rpcPardownResponse").click(function () {
            var currentElement = $("#myRpcModal").find("#rmiResponseParamTable > tbody > tr.outerTr.bg-success");
            var nextElement = currentElement.next();
            if (currentElement.next().next().next().length > 0) {
                currentElement.insertAfter(currentElement.next().next().next());
                nextElement.insertAfter(currentElement);
            }
        });

        //HTTP接口是否传值改变事件
        $("#myHttpModal").delegate("input[name='requiredsInputCheckBox']", "click", function () {
            if ($(this).prop('checked')) {
                $(this).prev("input[name='requiredsInput']").val("true");
            } else {
                $(this).prev("input[name='requiredsInput']").val("false");
            }
        });

        //RPC接口是否传值改变事件
        $("#myRpcModal").delegate("input[name='requiredsInputCheckBox']", "click", function () {
            if ($(this).prop('checked')) {
                $(this).prev("input[name='requiredsInput']").val("true");
            } else {
                $(this).prev("input[name='requiredsInput']").val("false");
            }
        });

        //HTTP接口是否JSON格式化传输改变事件
        $("#myHttpModal").delegate("input[name='jsontransformatsInputCheckBox']", "click", function () {
            if ($(this).prop("checked")) {
                $(this).prev("input[name='jsontransformatsInput']").val("true");
            } else {
                $(this).prev("input[name='jsontransformatsInput']").val("false");
            }
        });

        //RPC接口是否JSON格式化传输改变事件
        $("#myRpcModal").delegate("input[name='jsontransformatsInputCheckBox']", "click", function () {
            if ($(this).prop("checked")) {
                $(this).prev("input[name='jsontransformatsInput']").val("true");
            } else {
                $(this).prev("input[name='jsontransformatsInput']").val("false");
            }
        });


        //新增HTTP接口
        $("#addHttpInterfaceBtn").click(function () {
            $("#myHttpModalLabel").html("新增HTTP接口");
            $("#myHttpModal").find("tbody").empty();
            $("#interfaceHttpIdInput").val("");
            document.getElementById('newInterfaceHttpForm').reset();
        });

        //新增RPC接口
        $("#addRpcInterfaceBtn").click(function () {
            $("#myRpcModalLabel").html("新增RPC接口");
            $("#myRpcModal").find("tbody").empty();
            $("#interfaceRpcIdInput").val("");
            document.getElementById('newInterfaceRpcForm').reset();
        });

        //新增HTTP接口
        $("#newInterfaceHttpBtn").click(function () {
            if ($("#interfaceHttpIdInput").val() == null || $("#interfaceHttpIdInput").val() == "") {
                $.post("${ctxPath}/rest/interfaceHttp/insertInterfaceHttp", $("#newInterfaceHttpForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myHttpModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceHttp/updateInterfaceHttp", $("#newInterfaceHttpForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myHttpModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }
        });

        //新增RPC接口
        $("#newInterfaceRpcBtn").click(function () {
            if ($("#interfaceRpcIdInput").val() == null || $("#interfaceRpcIdInput").val() == "") {
                $.post("${ctxPath}/rest/interfaceRpc/insertInterfaceRpc", $("#newInterfaceRpcForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myRpcModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceRpc/updateInterfaceRpc", $("#newInterfaceRpcForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myRpcModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }
        });

        //HTTP接口编辑事件
        $("#httpTableTBody").delegate("span.btn-success-glyphicon", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#myHttpModalLabel").html("编辑HTTP接口");
            $("#interfaceHttpIdInput").val(interfaceId);
            $("#myHttpModal").find("tbody").empty();
            $.post("${ctxPath}/rest/interfaceHttp/findInterfaceHttp", {"interfaceId": interfaceId}, function (data) {
                $("#myHttpModal").find("select[name='protocolInput']").val(data.protocol);
                $("#myHttpModal").find("input[name='methodInput']").val(data.method);
                var methodCheckboxElements = $("input[name='methodInputCheckBox']");
                for (var i = 0; i < methodCheckboxElements.length; i++) {
                    if (data.method.indexOf($(methodCheckboxElements[i]).val()) > -1) {
                        $(methodCheckboxElements[i]).prop("checked", true);
                    }
                }
                $("#myHttpModal").find("select[name='transformatInput']").val(data.transformat);
                $("#myHttpModal").find("select[name='categoryInput']").val(data.category);
                $("#myHttpModal").find("input[name='addressInput']").val(data.address);
                $("#myHttpModal").find("input[name='nameInput']").val(data.name);
                $("#myHttpModal").find("textarea[name='descriptionInput']").val(data.description);

                //请求头部
                $("#myHttpModal").find("#headerParamTable > tbody").handlebars($("#httpHeaderParamEditTemplate"), data);

                //请求参数
                $("#myHttpModal").find("#requestParamTable > tbody").handlebars($("#httpRequestParamEditTemplate"), data);

                //响应参数
                $("#myHttpModal").find("#responseParamTable > tbody").handlebars($("#httpResponseParamEditTemplate"), data);

                //结果示例
                if (data.interfaceResultEntities != null && data.interfaceResultEntities.length > 0) {
                    for (var i = 0; i < data.interfaceResultEntities.length; i++) {
                        if (data.interfaceResultEntities[i].exampleType == "NORMAL") {
                            $("#myHttpModal").find("textarea[name='returnExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myHttpModal").find("textarea[name='returnExampleAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        } else {
                            $("#myHttpModal").find("textarea[name='exceptionExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myHttpModal").find("textarea[name='exceptionAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        }
                    }
                }
            }, "json");
        });

        //RPC接口编辑事件
        $("#rpcTableTBody").delegate("span.btn-success-glyphicon", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#myRpcModalLabel").html("编辑RPC接口");
            $("#interfaceRpcIdInput").val(interfaceId);
            $("#myRpcModal").find("tbody").empty();
            $.post("${ctxPath}/rest/interfaceRpc/findInterfaceRpc", {"interfaceId": interfaceId}, function (data) {
                $("#myRpcModal").find("select[name='categoryInput']").val(data.category);
                $("#myRpcModal").find("input[name='classNameInput']").val(data.className);
                $("#myRpcModal").find("input[name='methodNameInput']").val(data.methodName);
                $("#myRpcModal").find("input[name='nameInput']").val(data.name);
                $("#myRpcModal").find("textarea[name='functionDescribeInput']").val(data.functionDescribe);
                $("#myRpcModal").find("input[name='returnTypeInput']").val(data.returnType);
                $("#myRpcModal").find("input[name='methodParamInput']").val(data.methodParam);

                //请求参数
                $("#myRpcModal").find("#rmiParamTable > tbody").handlebars($("#rpcRequestParamEditTemplate"), data);

                //响应参数
                $("#myRpcModal").find("#rmiResponseParamTable > tbody").handlebars($("#rpcResponseParamEditTemplate"), data);

                //结果示例
                if (data.interfaceResultEntities != null && data.interfaceResultEntities.length > 0) {
                    for (var i = 0; i < data.interfaceResultEntities.length; i++) {
                        if (data.interfaceResultEntities[i].exampleType == "NORMAL") {
                            $("#myRpcModal").find("textarea[name='returnExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myRpcModal").find("textarea[name='returnExampleAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        } else {
                            $("#myRpcModal").find("textarea[name='exceptionExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myRpcModal").find("textarea[name='exceptionAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        }
                    }
                }
            }, "json");
        });

        //HTTP接口删除确定事件事件 | RPC接口删除确定事件事件
        $("#deleteInterfaceModal").delegate("button.btn-primary", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            var interfaceType = $(this).attr("data-interfacetype");
            if (interfaceType == "HTTP") {
                $.post("${ctxPath}/rest/interfaceHttp/deleteInterfaceHttp", {"interfaceId": interfaceId}, function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#deleteInterfaceModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceRpc/deleteInterfaceRpc", {"interfaceId": interfaceId}, function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#deleteInterfaceModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }

        });



        //HTTP请求头部预览复杂类型是否展示详情控制事件
        $("#myHttpPreviewModal").delegate("#myHttpPreviewHeaderTBody .glyphicon-chevron-down", "click", function () {
            $(this).parent().parent().parent().next().hide();
            $(this).removeClass("glyphicon-chevron-down");
            $(this).addClass("glyphicon-chevron-up");
        });

        $("#myHttpPreviewModal").delegate("#myHttpPreviewHeaderTBody .glyphicon-chevron-up", "click", function () {
            $(this).parent().parent().parent().next().show();
            $(this).removeClass("glyphicon-chevron-up");
            $(this).addClass("glyphicon-chevron-down");
        });

