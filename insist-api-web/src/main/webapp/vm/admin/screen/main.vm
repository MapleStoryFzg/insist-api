    <div class="row">
        <div class="col-sm-12">
            <div class="form-group">
                <table class="table table-bordered" style="table-layout: fixed">
                    <caption>参数说明</caption>
                    <thead>
                    <tr style="background-color: #B0B0B0">
                        <th>名称</th>
                        <th>类型</th>
                        <th>说明</th>
                        <th>是否必填</th>
                        <th width="18%">JSON格式化传输</th>
                        <th>示例</th>
                        <th>默认值</th>
                    </tr>
                    </thead>
                    <tbody id="myRpcPreviewTBody">
                    {{# if interfaceParamEntities}}
                    {{#interfaceParamEntities}}
                    <tr style="background-color: #F0F0F0">
                        <td style="word-wrap: break-word">{{paramName}}</td>
                        <td>{{paramType}}</td>
                        <td style="word-wrap: break-word">{{paramDescribe}}</td>
                        <td>{{required}}</td>
                        <td>{{jsontransformat}}</td>
                        <td style="word-wrap: break-word">{{example}}</td>
                        <td style="word-wrap: break-word">{{defaultValue}}</td>
                    </tr>
                    <tr {{#compareParamType paramType
                    'object'}} style="display:table-row" {{else}} {{#compareParamType paramType
                    'json'}} style="display:table-row" {{else}} style="display:none" {{/compareParamType}}
                    {{/compareParamType}}>
                    <td colspan="7">
                        <pre>{{exampleDetail}}</pre>
                        <br/>
                        <pre>{{defaultValueDetail}}</pre>
                    </td>
                    </tr>
                    {{/interfaceParamEntities}}
                    {{else}}
                    <tr style="background-color: #F0F0F0">
                        <td class='text-center' colspan='7'>暂无信息！</td>
                    </tr>
                    {{/if}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="form-group">
                <ul id="myRpcPreviewTab" class="nav nav-tabs">
                    <li class="active">
                        <a href="#returnExampleRpcPreview" data-toggle="tab">
                            返回示例
                        </a>
                    </li>
                    <li>
                        <a href="#exceptionExampleRpcPreview" data-toggle="tab">
                            异常示例
                        </a>
                    </li>
                </ul>
                <div id="myRpcPreviewTabContent" class="tab-content">
                    {{# if interfaceResultEntities}}
                    {{#interfaceResultEntities}}
                    {{#compareExampleType exampleType "NORMAL" }}
                    <div class="tab-pane fade in active" id="returnExampleRpcPreview">
                        <br/>
                        <div class="form-group">
                            <h6>返回示例</h6>
                            <pre id="rpcReturnExampleView">{{# if exampleContent}}{{exampleContent}}{{else}}暂无返回示例！{{/if}}</pre>
                        </div>
                        <br/>
                        <div class="form-group">
                            <h6>注意事项</h6>
                            <pre id="rpcReturnAttentionMattersView">{{# if attentionMatters}}{{attentionMatters}}{{else}}暂无注意事项！{{/if}}</pre>
                        </div>
                    </div>
                    {{else}}
                    <div class="tab-pane fade" id="exceptionExampleRpcPreview">
                        <br/>
                        <div class="form-group">
                            <h6>异常示例</h6>
                            <pre id="rpcExceptionExampleView">{{# if exampleContent}}{{exampleContent}}{{else}}暂无返回示例！{{/if}}</pre>
                        </div>
                        <br/>
                        <div class="form-group">
                            <h6>注意事项</h6>
                            <pre id="rpcExceptionAttentionMattersView">{{# if attentionMatters}}{{attentionMatters}}{{else}}暂无注意事项！{{/if}}</pre>
                        </div>
                    </div>
                    {{/compareExampleType}}
                    {{/interfaceResultEntities}}
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
</script>


<script type="text/javascript">
    $(function () {

        //HTTP请求参数类型选择事件
        $("#myHttpModal").find("#requestParamTable>tbody").delegate("select[name='paramTypesInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //RPC请求参数类型选择事件
        $("#myRpcModal").find("#rmiParamTable>tbody").delegate("select[name='paramTypesInput']", "change", function () {
            if ($(this).val() == "object" || $(this).val() == "json") {
                $(this).parent().parent().next().css("display", "table-row");
            } else {
                $(this).parent().parent().next().css("display", "none");
            }
        });

        //HTTP接口添加参数
        $("#httpAddRequestParam").click(function () {
            $("#myHttpModal").find("tbody").append($("#httpAddRequestParamTemplate").html());
        });

        //Rpc接口添加参数
        $("#rpcAddRequestParam").click(function () {
            $("#myRpcModal").find("tbody").append($("#rpcAddRequestParamTemplate").html());
        });

        //HTTP接口行选中事件
        $("#myHttpModal").delegate("tbody > tr", "click", function () {
            $("#myHttpModal").find("tbody > tr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //RPC接口行选中事件
        $("#myRpcModal").delegate("tbody > tr", "click", function () {
            $("#myRpcModal").find("tbody > tr").removeClass("bg-success");
            $(this).addClass("bg-success");
        });

        //Http接口删除行事件
        $("#myHttpModal").delegate(".glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().remove();
        });

        //RPC接口删除行事件
        $("#myRpcModal").delegate(".glyphicon-trash", "click", function () {
            $(this).parent().parent().parent().remove();
        });

        //Http接口上移事件
        $("#httpParup").click(function () {
            var currentElement = $("#myHttpModal").find("tbody > tr.bg-success");
            if (currentElement.prev().length > 0) {
                currentElement.prev().insertAfter(currentElement);
            }
        });

        //RPC接口上移事件
        $("#rpcParup").click(function () {
            var currentElement = $("#myRpcModal").find("tbody > tr.bg-success");
            if (currentElement.prev().length > 0) {
                currentElement.prev().insertAfter(currentElement);
            }
        });

        //Http接口下移事件
        $("#httpPardown").click(function () {
            var currentElement = $("#myHttpModal").find("tbody > tr.bg-success");
            if (currentElement.next().length > 0) {
                currentElement.insertAfter(currentElement.next());
            }
        });

        //RPC接口下移事件
        $("#rpcPardown").click(function () {
            var currentElement = $("#myRpcModal").find("tbody > tr.bg-success");
            if (currentElement.next().length > 0) {
                currentElement.insertAfter(currentElement.next());
            }
        });

        //HTTP接口是否传值改变事件
        $("#myHttpModal").delegate("input[name='requiredsInputCheckBox']", "click", function () {
            if ($(this).prop('checked')) {
                $(this).prev("input[name='requiredsInput']").val("true");
            } else {
                $(this).prev("input[name='requiredsInput']").val("false");
            }
        });

        //RPC接口是否传值改变事件
        $("#myRpcModal").delegate("input[name='requiredsInputCheckBox']", "click", function () {
            if ($(this).prop('checked')) {
                $(this).prev("input[name='requiredsInput']").val("true");
            } else {
                $(this).prev("input[name='requiredsInput']").val("false");
            }
        });

        //HTTP接口是否JSON格式化传输改变事件
        $("#myHttpModal").delegate("input[name='jsontransformatsInputCheckBox']", "click", function () {
            if ($(this).prop("checked")) {
                $(this).prev("input[name='jsontransformatsInput']").val("true");
            } else {
                $(this).prev("input[name='jsontransformatsInput']").val("false");
            }
        });

        //RPC接口是否JSON格式化传输改变事件
        $("#myRpcModal").delegate("input[name='jsontransformatsInputCheckBox']", "click", function () {
            if ($(this).prop("checked")) {
                $(this).prev("input[name='jsontransformatsInput']").val("true");
            } else {
                $(this).prev("input[name='jsontransformatsInput']").val("false");
            }
        });


        //新增HTTP接口
        $("#addHttpInterfaceBtn").click(function () {
            $("#myHttpModalLabel").html("新增HTTP接口");
            $("#myHttpModal").find("tbody").empty();
            $("#interfaceHttpIdInput").val("");
            document.getElementById('newInterfaceHttpForm').reset();
        });

        //新增RPC接口
        $("#addRpcInterfaceBtn").click(function () {
            $("#myrpcmodallabel").html("新增RPC接口");
            $("#myRpcModal").find("tbody").empty();
            $("#interfaceRpcIdInput").val("");
            document.getElementById('newInterfaceRpcForm').reset();
        });

        //新增HTTP接口
        $("#newInterfaceHttpBtn").click(function () {
            if ($("#interfaceHttpIdInput").val() == null || $("#interfaceHttpIdInput").val() == "") {
                $.post("${ctxPath}/rest/interfaceHttp/insertInterfaceHttp", $("#newInterfaceHttpForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myHttpModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceHttp/updateInterfaceHttp", $("#newInterfaceHttpForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myHttpModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }
        });

        //新增RPC接口
        $("#newInterfaceRpcBtn").click(function () {
            if ($("#interfaceRpcIdInput").val() == null || $("#interfaceRpcIdInput").val() == "") {
                $.post("${ctxPath}/rest/interfaceRpc/insertInterfaceRpc", $("#newInterfaceRpcForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myRpcModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceRpc/updateInterfaceRpc", $("#newInterfaceRpcForm").serialize(), function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#myRpcModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }
        });

        //HTTP接口列表展示
        $.post("${ctxPath}/rest/interfaceHttp/findInterfaceHttpList", {"appId": "$!{appDO.appId}"}, function (data) {
            $("#httpTableTBody").handlebars($("#httpInterfaceTemplate"), data);
        }, "json");

        //RPC接口列表展示
        $.post("${ctxPath}/rest/interfaceRpc/findInterfaceRpcList", {"appId": "$!{appDO.appId}"}, function (data) {
            $("#rpcTableTBody").handlebars($("#rpcInterfaceTemplate"), data);
        }, "json");

        //HTTP接口编辑事件
        $("#httpTableTBody").delegate("button.btn-success", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#myHttpModalLabel").html("编辑HTTP接口");
            $("#interfaceHttpIdInput").val(interfaceId);
            $("#myHttpModal").find("tbody").empty();
            $.post("${ctxPath}/rest/interfaceHttp/findInterfaceHttp", {"interfaceId": interfaceId}, function (data) {
                $("#myHttpModal").find("select[name='protocolInput']").val(data.protocol);
                $("#myHttpModal").find("select[name='methodInput']").val(data.method);
                $("#myHttpModal").find("select[name='transformatInput']").val(data.transformat);
                $("#myHttpModal").find("input[name='addressInput']").val(data.address);
                $("#myHttpModal").find("input[name='nameInput']").val(data.name);
                $("#myHttpModal").find("textarea[name='descriptionInput']").val(data.description);
                $("#myHttpModal").find("input[name='mockAddressInput']").val(data.mockAddress);
                $("#myHttpModal").find("input[name='devAddressInput']").val(data.devAddress);
                $("#myHttpModal").find("input[name='sitAddressInput']").val(data.sitAddress);
                $("#myHttpModal").find("input[name='uatAddressInput']").val(data.uatAddress);
                $("#myHttpModal").find("input[name='prdAddressInput']").val(data.prdAddress);

                //请求参数
                $("#myHttpModal").find("tbody").handlebars($("#httpRequestParamEditTemplate"), data);

                //结果示例
                if (data.interfaceResultEntities != null && data.interfaceResultEntities.length > 0) {
                    for (var i = 0; i < data.interfaceResultEntities.length; i++) {
                        if (data.interfaceResultEntities[i].exampleType == "NORMAL") {
                            $("#myHttpModal").find("textarea[name='returnExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myHttpModal").find("textarea[name='returnExampleAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        } else {
                            $("#myHttpModal").find("textarea[name='exceptionExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myHttpModal").find("textarea[name='exceptionAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        }
                    }
                }
            }, "json");
        });

        //RPC接口编辑事件
        $("#rpcTableTBody").delegate("button.btn-success", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#myRpcModalLabel").html("编辑RPC接口");
            $("#interfaceRpcIdInput").val(interfaceId);
            $("#myRpcModal").find("tbody").empty();
            $.post("${ctxPath}/rest/interfaceRpc/findInterfaceRpc", {"interfaceId": interfaceId}, function (data) {
                $("#myRpcModal").find("input[name='classNameInput']").val(data.className);
                $("#myRpcModal").find("input[name='methodNameInput']").val(data.methodName);
                $("#myRpcModal").find("input[name='nameInput']").val(data.name);
                $("#myRpcModal").find("textarea[name='functionDescribeInput']").val(data.functionDescribe);
                $("#myRpcModal").find("input[name='returnTypeInput']").val(data.returnType);
                $("#myRpcModal").find("input[name='methodParamInput']").val(data.methodParam);

                //请求参数
                $("#myRpcModal").find("tbody").handlebars($("#rpcRequestParamEditTemplate"), data);

                //结果示例
                if (data.interfaceResultEntities != null && data.interfaceResultEntities.length > 0) {
                    for (var i = 0; i < data.interfaceResultEntities.length; i++) {
                        if (data.interfaceResultEntities[i].exampleType == "NORMAL") {
                            $("#myRpcModal").find("textarea[name='returnExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myRpcModal").find("textarea[name='returnExampleAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        } else {
                            $("#myRpcModal").find("textarea[name='exceptionExampleContent']").val(data.interfaceResultEntities[i].exampleContent);
                            $("#myRpcModal").find("textarea[name='exceptionAttentionMatters']").val(data.interfaceResultEntities[i].attentionMatters);
                        }
                    }
                }
            }, "json");
        });

        //HTTP接口删除事件
        $("#httpTableTBody").delegate("button.btn-danger", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#deleteInterfaceModal").find("button.btn-primary").attr("data-interfaceid", interfaceId);
            $("#deleteInterfaceModal").find("button.btn-primary").attr("data-interfacetype", "HTTP");

        });

        //RPC接口删除事件
        $("#rpcTableTBody").delegate("button.btn-danger", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $("#deleteInterfaceModal").find("button.btn-primary").attr("data-interfaceid", interfaceId);
            $("#deleteInterfaceModal").find("button.btn-primary").attr("data-interfacetype", "RPC");

        });

        //HTTP接口删除确定事件事件 | RPC接口删除确定事件事件
        $("#deleteInterfaceModal").delegate("button.btn-primary", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            var interfaceType = $(this).attr("data-interfacetype");
            if (interfaceType == "HTTP") {
                $.post("${ctxPath}/rest/interfaceHttp/deleteInterfaceHttp", {"interfaceId": interfaceId}, function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#deleteInterfaceModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            } else {
                $.post("${ctxPath}/rest/interfaceRpc/deleteInterfaceRpc", {"interfaceId": interfaceId}, function (data) {
                    console.log(data);
                    if (data.success) {
                        $("#deleteInterfaceModal").modal("hide");
                        window.location.reload();
                    }
                }, "json");
            }

        });

        //HTTP接口预览
        $("#httpTableTBody").delegate("button.btn-warning", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $.post("${ctxPath}/rest/interfaceHttp/findInterfaceHttp", {"interfaceId": interfaceId}, function (data) {
                $("#myHttpPreviewModal").find(".modal-body").handlebars($("#myHttpPreviewModalTemplate"), data);
            }, "json");
        });

        $("#myHttpPreviewModal").delegate(".glyphicon-chevron-down", "click", function () {
            $(this).parent().parent().parent().next().hide();
            $(this).removeClass("glyphicon-chevron-down");
            $(this).addClass("glyphicon-chevron-up");
        });

        $("#myHttpPreviewModal").delegate(".glyphicon-chevron-up", "click", function () {
            $(this).parent().parent().parent().next().show();
            $(this).removeClass("glyphicon-chevron-up");
            $(this).addClass("glyphicon-chevron-down");
        });


        //RPC接口预览
        $("#rpcTableTBody").delegate("button.btn-warning", "click", function () {
            var interfaceId = $(this).attr("data-interfaceid");
            $.post("${ctxPath}/rest/interfaceRpc/findInterfaceRpc", {"interfaceId": interfaceId}, function (data) {
                $("#myRpcPreviewModal").find(".modal-body").handlebars($("#myRpcPreviewModalTemplate"), data);
            }, "json");
        });


    });
</script>
